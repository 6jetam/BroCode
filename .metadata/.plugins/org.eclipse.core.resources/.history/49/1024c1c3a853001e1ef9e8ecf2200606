import java.util.Scanner;

public class Skusaj {

	public static void main(String[] args) {
		Scanner vstup = new Scanner (System.in);
		System.out.println("Napis svoje meno:");
		String name = vstup.nextLine();
		System.out.println("Napis svoj vek:");
		int age = vstup.nextInt();
		System.out.println("Tvoja vyska je?");
		double vyska = vstup.nextDouble();
		
		System.out.println("Tvoje meno je " + name + " a vek " + age + " rokov a " + vyska + " cm");

	}

}
